<h1>Listing survey1s</h1>

<table>
  <tr>
    <th>Answer</th>
    <th>Time</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @survey1s.each do |survey1| %>
  <tr>
    <td><%= survey1.answer %></td>
    <td><%= survey1.time %></td>
    <td><%= link_to 'Show', survey1 %></td>
    <td><%= link_to 'Edit', edit_survey1_path(survey1) %></td>
    <td><%= link_to 'Destroy', survey1, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

